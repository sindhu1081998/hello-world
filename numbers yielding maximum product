#include<stdio.h>

#include <stdlib.h>


int main()
{

int n,i,j,b,max,c,d;

scanf("%d",&n);

int a[n];

for(i=0;i<n;i++)

{
    
scanf("%d ",&a[i]);

}

max=a[0]*a[1];

for(i=1;i<n;i++)

{
    for(j=i+1;j<n;j++)
    
{
        b=a[i]*a[j];
       
 if(b>max)
        
{
            
max=b;
            
c=a[i];
            
d=a[j];
            
        
}
    
}

}
printf("%d %d",c,d);


}
